{
    "compilerOptions": {
        "incremental": true,
        "target": "es5",
        "jsx": "react",
        "module": "amd",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "lib": [
            "es2015.iterable",
            "es5",
            "es6",
            "es2015",
            "es2017",
            "dom",
            "es2015.core",
            "es2015.promise"
        ],
        "types": [
            "ace",
            "ckeditor",
            "immutable",
            "react",
            "react-dom",
            "react-dropzone",
            "requirejs",
            "@editorjs/editorjs"
        ]
    }
}